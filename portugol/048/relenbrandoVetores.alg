algoritmo "relembrandoVetores"
var n1, n2, n3, contador: inteiro
    v: vetor[1..3] de inteiro

inicio
      n1 <- 3
      n2 <- 4
      n3 <- 1
      
      Para contador <- 1 ate 3 faca
         v[contador] <- contador
         Escreva(v[contador])
      fimpara
      
      Escreva(n1, n2, n3)
fimalgoritmo
