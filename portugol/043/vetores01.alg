algoritmo "vetores01"
var n1, i: inteiro
    n2, n3, n4: inteiro
    n: vetor[1..4] de inteiro

inicio
      n1 <- 1
      n2 <- 0
      n3 <- 5
      n4 <- 3
      
      n[1] <- 3
      n[2] <- 2
      n[3] <- 10
      n[4] <- 21
      Escreval(n[1])
      
      Para i <- 1 ate 4 faca
         Escreval(n[i])
      fimpara
fimalgoritmo
