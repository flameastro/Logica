algoritmo "estruturasCondicionais01"
var dinheiro: real

inicio
      Escreva("Quanto dinheiro você tem? R$")
      Leia(dinheiro)
      
      se (dinheiro > 5000) então
         Escreva("Eu CONSIGO fazer uma viagem para a Disney")
      senao
         Escreva("Eu NÃO CONSIGO fazer uma viagem para a Disney")
      fimse
fimalgoritmo
